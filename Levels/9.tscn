[sub_resource type="GDScript" id=5]

script/source = "extends Tween

onready var pf = $\"..\"

var rotation = 90
onready var pos = pf.position
var pos_change = Vector2(1000,0)

func _ready():
	set_active(true)
	interpolate_property(pf, \"rotation\", 0, 90, 5, Tween.TRANS_LINEAR, Tween.EASE_IN)
	interpolate_property(pf, \"position\", pf.position, pf.position + pos_change, 5, Tween.TRANS_LINEAR, Tween.EASE_IN)
	start()

func _on_Tween_tween_completed(object, key):
	reset(object,key)
	if key == \"rotation\":
		rotation *= -1
		interpolate_property(object, key, 0, rotation, 5, Tween.TRANS_LINEAR, Tween.EASE_IN)
	elif key == \"position\":
		pos *= -1
		interpolate_property(object, key, pf.position, pf.position + pos_change, 5, Tween.TRANS_LINEAR, Tween.EASE_IN)
	start()
	
	
	
	
"

